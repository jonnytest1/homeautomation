  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "launch local receiver",
      //"program": "${workspaceFolder}\\index.ts",
      "autoAttachChildProcesses": true,
      "cwd": "${workspaceFolder}",
      "runtimeArgs": [
        "-r",
        "ts-node/register"
      ],
      "env": {
        "profile": "DEV"
      },
      "args": [
        "${workspaceFolder}\\index.ts"
      ],
      "restart": true,
    },
    {
      "type": "node",
      "request": "launch",
      "name": "prod launch local receiver",
      "console": "internalConsole",
      //"program": "${workspaceFolder}\\index.ts",
      "autoAttachChildProcesses": true,
      "cwd": "${workspaceFolder}",
      "runtimeArgs": [
        "-r",
        "ts-node/register"
      ],
      "env": {
        "profile": "PROD"
      },
      "args": [
        "${workspaceFolder}\\index.ts"
      ],
      "restart": true,
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to prod receiver",
      "address": "127.0.0.1",
      "port": 29220
    },
  ]
}