{
  "compileOnSave": false,
  "angularCompilerOptions": {
    "strictTemplates": true
  },
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "node",
    "strictNullChecks": false,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "downlevelIteration": true,
    "target": "es2020",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom",
      "ES2019",
      "ES2019.Object",
      "ES2015",
      "ESNext",
      "DOM.Iterable"
    ],
  }
}